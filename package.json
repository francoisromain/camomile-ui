{
  "name": "camomile-ui",
  "version": "0.1.0",
  "description": "UI Components library for Camomile",
  "author": "Francois Romain <francoisromain@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/francoisromain/camomile-ui"
  },
  "scripts": {
    "cli": "cross-env NODE_ENV=dev webpack-dev-server --inline --hot --config build/webpack.config.cli",
    "dist": "cross-env NODE_ENV=production node build/dist.js",
    "dev": "cross-env NODE_ENV=development node build/dev.js",
    "styleguide": "vue-styleguidist server",
    "styleguide:build": "vue-styleguidist build"
  },
  "dependencies": {
    "camomile-client": "^0.8.0",
    "babel-polyfill": "^6.26.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "axios": "^0.17.1",
