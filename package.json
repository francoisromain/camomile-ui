    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "copy-webpack-plugin": "^4.2.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-html": "^4.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-exclude-assets-plugin": "0.0.5",
    "html-webpack-plugin": "^2.30.1",
    "imports-loader": "^0.7.1",
    "json-loader": "^0.5.7",
    "normalize.css": "^7.0.0",
    "object-fit-images": "^3.2.3",
    "pixrem": "^4.0.1",
    "pleeease-filters": "^4.0.0",
    "postcss": "^6.0.14",
    "postcss-attribute-case-insensitive": "^2.0.0",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-button": "^0.2.8",
    "postcss-calc": "^6.0.1",
    "postcss-color-function": "^4.0.1",
    "postcss-color-gray": "^4.0.0",
    "postcss-color-hex-alpha": "^3.0.0",
    "postcss-color-hsl": "^2.0.0",
    "postcss-color-hwb": "^3.0.0",
    "postcss-color-rgb": "^2.0.0",
    "postcss-color-rgba-fallback": "^3.0.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^6.2.0",
    "postcss-custom-selectors": "^4.0.1",
    "postcss-font-family-system-ui": "^2.0.1",
    "postcss-font-variant": "^3.0.0",
    "postcss-grid-fluid": "^0.1.19",
    "postcss-grid-system": "^0.4.11",
    "postcss-image-set-polyfill": "^0.4.4",
    "postcss-import": "^11.0.0",
    "postcss-initial": "^2.0.0",
    "postcss-inline-svg": "^3.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-media-minmax": "^3.0.0",
    "postcss-nesting": "^4.2.1",
    "postcss-normalize": "^4.0.0",
    "postcss-pseudo-class-any-link": "^4.0.0",
    "postcss-pseudoelements": "^5.0.0",
    "postcss-replace-overflow-wrap": "^2.0.0",
    "postcss-reporter": "^5.0.0",
    "postcss-selector-matches": "^3.0.1",
    "postcss-selector-not": "^3.0.1",
    "postcss-typescale": "^0.1.10",
    "postcss-url": "^7.1.2",
    "progress-bar-webpack-plugin": "^1.10.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-processor-html": "^1.0.0",
    "url-loader": "^0.6.2",
    "vue": "^2.5.3",
    "vue-hot-reload-api": "^2.2.3",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.5.0",
    "vue-style-loader": "^3.0.3",
    "vue-styleguidist": "^1.3.3",
    "vue-template-compiler": "^2.5.3",
    "vuex": "^3.0.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.1",
    "webpack-monitor": "^1.0.13"
  },
  "eslintConfig": {
    "extends": "standard",
    "env": {
      "browser": true
    },
    "plugins": [
      "html",
      "compat"
    ],
    "rules": {
      "compat/compat": 2,
      "arrow-parens": 0,
      "generator-star-spacing": 0
    }
  },
  "stylelint": {
    "processors": [
      "stylelint-processor-html"
    ],
    "extends": "stylelint-config-standard",
    "rules": {
      "property-no-unknown": [
        true,
        {
          "ignoreProperties": [
            "typescale",
            "gf",
            "/button-/"
          ]
        }
      ]
    },
    "ignoreFiles": [
      "./dist/**/*"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
