    "eslint": "^4.13.1",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-exclude-assets-plugin": "0.0.5",
    "html-webpack-plugin": "^2.30.1",
    "imports-loader": "^0.7.1",
    "jest": "^22.0.3",
    "jest-serializer-vue": "^0.3.0",
    "jest-vue-preprocessor": "^1.3.1",
    "json-loader": "^0.5.7",
    "object-fit-images": "^3.2.3",
    "pixrem": "^4.0.1",
    "pleeease-filters": "^4.0.0",
    "plyr": "^2.0.18",
    "postcss": "^6.0.14",
    "postcss-attribute-case-insensitive": "^2.0.0",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-button": "^0.3.7",
    "postcss-calc": "^6.0.1",
    "postcss-color-function": "^4.0.1",
    "postcss-color-gray": "^4.1.0",
    "postcss-color-hex-alpha": "^3.0.0",
    "postcss-color-hsl": "^2.0.0",
    "postcss-color-hwb": "^3.0.0",
    "postcss-color-rgb": "^2.0.0",
    "postcss-color-rgba-fallback": "^3.0.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^6.2.0",
    "postcss-custom-selectors": "^4.0.1",
    "postcss-font-family-system-ui": "^2.1.1",
    "postcss-font-variant": "^3.0.0",
    "postcss-grid-fluid": "^0.1.19",
    "postcss-grid-system": "^0.4.11",
    "postcss-image-set-polyfill": "^0.4.4",
    "postcss-import": "^11.0.0",
    "postcss-initial": "^2.0.0",
    "postcss-inline-svg": "^3.0.0",
    "postcss-loader": "^2.0.9",
    "postcss-media-minmax": "^3.0.0",
    "postcss-nesting": "^4.2.1",
    "postcss-normalize": "^4.0.0",
    "postcss-pseudo-class-any-link": "^4.0.0",
    "postcss-pseudoelements": "^5.0.0",
    "postcss-replace-overflow-wrap": "^2.0.0",
    "postcss-reporter": "^5.0.0",
    "postcss-selector-matches": "^3.0.1",
    "postcss-selector-not": "^3.0.1",
    "postcss-typescale": "^0.1.10",
    "postcss-url": "^7.3.0",
    "progress-bar-webpack-plugin": "^1.10.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.52.3",
    "rollup-plugin-buble": "^0.18.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.5.5",
    "rollup-plugin-vue": "^3.0.0",
    "style-loader": "^0.19.1",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-processor-html": "^1.0.0",
    "url-loader": "^0.6.2",
    "vue-hot-reload-api": "^2.2.4",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.6.0",
    "vue-server-renderer": "^2.5.13",
    "vue-style-loader": "^3.0.3",
    "vue-styleguidist": "^1.3.9",
    "vue-template-compiler": "^2.5.13",
    "vue-test-utils": "^1.0.0-beta.8",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1",
    "webpack-monitor": "^1.0.14"
  },
  "eslintConfig": {
    "extends": "standard",
    "env": {
      "browser": true
    },
    "plugins": ["html", "compat"],
    "rules": {
      "compat/compat": 2,
      "arrow-parens": 0,
      "generator-star-spacing": 0
    }
  },
  "stylelint": {
    "processors": ["stylelint-processor-html"],
    "extends": "stylelint-config-standard",
    "rules": {
      "property-no-unknown": [
        true,
        {
          "ignoreProperties": ["typescale", "gf", "/button-/"]
        }
      ]
    },
    "ignoreFiles": ["./dist/**/*"]
  },
  "browserslist": [
    "Chrome >= 60",
    "Safari >= 10.1",
    "iOS >= 10.3",
    "Firefox >= 54",
    "Edge >= 15"
  ],
  "jest": {
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.runtime.min.js",
      "^.+\\.(css)$": "./src/__mocks__/css.js"
    },
    "moduleFileExtensions": ["js", "json", "vue"],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "jest-vue-preprocessor"
    },
    "mapCoverage": true,
    "snapshotSerializers": ["jest-serializer-vue"],
    "roots": ["./src/"],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "postcss": {
    "plugins": {
      "postcss-normalize": {},
      "postcss-import": {},
      "postcss-custom-properties": {},
      "postcss-calc": {},
      "postcss-image-set-polyfill": {},
      "postcss-nesting": {},
      "postcss-custom-media": {},
      "postcss-media-minmax": {},
      "postcss-custom-selectors": {},
      "postcss-attribute-case-insensitive": {},
      "postcss-color-hwb": {},
      "postcss-color-hsl": {},
      "postcss-color-rgb": {},
      "postcss-color-gray": {},
      "postcss-color-hex-alpha": {},
      "postcss-color-function": {},
      "postcss-font-family-system-ui": {},
      "postcss-font-variant": {},
      "pleeease-filters": {},
      "postcss-initial": {},
      "pixrem": {},
      "postcss-pseudoelements": {},
      "postcss-selector-matches": {},
      "postcss-selector-not": {},
      "postcss-pseudo-class-any-link": {},
      "postcss-color-rgba-fallback": {},
      "postcss-replace-overflow-wrap": {},
      "postcss-typescale": {},
      "postcss-grid-system": {},
      "postcss-grid-fluid": {},
      "postcss-button": {},
      "postcss-inline-svg": {},
      "autoprefixer": {}
    }
  }
}
