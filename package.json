{
  "name": "@camomile/camomile-ui",
  "version": "0.1.5",
  "description": "UI Component library for Camomile",
  "license": "MIT",
  "homepage": "https://github.com/francoisromain/camomile-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/francoisromain/camomile-ui.git"
  },
  "author": {
    "name": "FranÃ§ois Romain",
    "email": "francoisromain@gmail.com",
    "url": "https://francoisromain.com"
  },
  "bugs": {
    "url": "https://github.com/francoisromain/camomile-ui/issues"
  },
  "directories": {
    "example": "example",
    "test": "test"
  },
  "main": "dist/camomile-ui.common.js",
  "module": "dist/camomile-ui.esm.js",
  "browser": "dist/camomile-ui.umd.js",
  "keywords": ["camomile", "ui", "collaborative", "annotation", "platform"],
  "scripts": {
    "jest": "jest --verbose --coverage --watch",
    "test": "jest --verbose && codecov",
    "dist": "cross-env NODE_ENV=production node build/webpack.dist.js",
    "dev": "cross-env NODE_ENV=development node build/webpack.dev.js",
    "build": "rollup -c build/rollup.config.dist.js",
    "styleguide":
      "vue-styleguidist --config build/webpack.styleguide.config.js server",
    "styleguide:build":
      "vue-styleguidist --config build/webpack.styleguide.config.js build"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "camomile-client": "^0.8.0",
    "vue": "^2.5.13",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.35",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.35",
